[mon.]
  key = {{ lookup('file', mon_key) }}
  caps mon = "allow *"

[client.admin]
  key = {{ lookup('file', admin_key) }}
  auid = 0
  caps mds = "allow *"
  caps mgr = "allow *"
  caps mon = "allow *"
  caps osd = "allow *"

[client.bootstrap-osd]
  key = {{ lookup('file', bootstrap_key) }}
  caps mon = "profile bootstrap-osd"
