apiVersion: v1
kind: Service
metadata:
  annotations:
    service.alpha.kubernetes.io/tolerate-unready-endpoints: "true"
  name: cockroach
spec:
  ports:
  - port:  26257
    protocol: TCP
    targetPort: grpc
    name: grpc
  - port: 80
    protocol: TCP
    targetPort: http
    name: http
  clusterIP: None
  selector:
    app: cockroach
